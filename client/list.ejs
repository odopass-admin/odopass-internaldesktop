		<script type="text/javascript">

        const { ipcRenderer } = require('electron');

        //0 --> DELETE THE ERRORS
         ipcRenderer.send('ejsData', {data:"fesflefle"});

         //--------------------------------------------------
        // 3 --> LOGIN FORM IF BUTTON PRESS AND NO SESSION
        //--------------------------------------------------
        function SwitchEnv(environnement) {
          console.log("test")
          ipcRenderer.send('switch_environnement', {environnement: environnement});

        }

				const notification = document.getElementById('notification');
				const message = document.getElementById('message');


				const restartButton = document.getElementById('restart-button');ipcRenderer.on('update_available', () => {
				  ipcRenderer.removeAllListeners('update_available');
				  message.innerText = 'A new update is available. Downloading now...';
				  notification.classList.remove('hidden');
				});


				ipcRenderer.on('check_update', () => {
				  ipcRenderer.removeAllListeners('check_update');
					message.innerText = 'Checking for update';
				  restartButton.classList.remove('hidden');
				  notification.classList.remove('hidden');
				});


				ipcRenderer.on('no_update_available', () => {
				  ipcRenderer.removeAllListeners('no_update_available');
					message.innerText = 'No update Available';
				  restartButton.classList.remove('hidden');
				  notification.classList.remove('hidden');
				});


				ipcRenderer.on('update_available', () => {
				  ipcRenderer.removeAllListeners('update_available');
					message.innerText = 'Update Available';
				  restartButton.classList.remove('hidden');
				  notification.classList.remove('hidden');
				});


				ipcRenderer.on('update_downloaded', () => {
				  ipcRenderer.removeAllListeners('update_downloaded');
				  message.innerText = 'Update Downloaded. It will be installed on restart. Restart now?';
				  restartButton.classList.remove('hidden');
				  notification.classList.remove('hidden');
				});


				function closeNotification() {
				  notification.classList.add('hidden');
				}

				function restartApp() {
				  ipcRenderer.send('restart_app');
				}

    </script>
